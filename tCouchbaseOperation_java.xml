<?xml version="1.0" encoding="UTF-8"?>
<COMPONENT>
	<HEADER AUTHOR="ijokarumawak" COMPATIBILITY="ALL" PLATEFORM="ALL"
		RELEASE_DATE="20120813A" SERIAL="" STARTABLE="false" STATUS="BETA"
		VERSION="0.1" DATA_AUTO_PROPAGATE="false"  
  SCHEMA_AUTO_PROPAGATE="false"
  HAS_CONDITIONAL_OUTPUTS="true">
		<SIGNATURE />
	</HEADER>
	<FAMILIES>
		<FAMILY>NoSQL/Couchbase</FAMILY>
	</FAMILIES>
	<DOCUMENTATION>
	    <URL>https://github.com/ijokarumawak/tCouchbaseOperation</URL> 
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1"
			MIN_INPUT="1" MIN_OUTPUT="0" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" FILTER="tCouchbaseConnection"
			NUM_ROW="1" />
		<PARAMETER NAME="OPERATION" FIELD="CLOSED_LIST" NUM_ROW="2">
			<ITEMS DEFAULT="GET">
				<ITEM NAME="GET" VALUE="GET" />
				<ITEM NAME="SET" VALUE="SET" />
				<ITEM NAME="DELETE" VALUE="DELETE" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER FIELD="TEXT" NAME="ID" NUM_ROW="4" REQUIRED="true" />

		<PARAMETER FIELD="TEXT" NAME="EXPIRATION" NUM_ROW="6"
			REQUIRED="true" SHOW_IF="(OPERATION == 'SET')">
			<DEFAULT>0</DEFAULT>
		</PARAMETER>

		<PARAMETER FIELD="TEXT" NAME="VALUE" NUM_ROW="7" REQUIRED="true"
			SHOW_IF="(OPERATION == 'SET')" />

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="9"/>
	</PARAMETERS>
	<ADVANCED_PARAMETERS>
	</ADVANCED_PARAMETERS>
	<CODEGENERATION />
	<RETURNS>
		<RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer" />
	</RETURNS>
</COMPONENT>

